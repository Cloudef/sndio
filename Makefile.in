all:
	cd libsndio && ${MAKE}
	cd aucat && ${MAKE}
	cd examples && ${MAKE}

install:
	cd libsndio && ${MAKE} install
	cd aucat && ${MAKE} install

uninstall:
	cd libsndio && ${MAKE} uninstall
	cd aucat && ${MAKE} uninstall

clean:
	cd libsndio && ${MAKE} clean
	cd aucat && ${MAKE} clean
	cd examples && ${MAKE} clean

distclean:	clean
	rm -f Makefile libsndio/Makefile aucat/Makefile examples/Makefile

